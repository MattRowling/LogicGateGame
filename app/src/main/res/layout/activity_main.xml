<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    app:barrierMargin="10dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/mainBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/levelButton12"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/round_button"
        android:backgroundTint="#000000"
        android:cropToPadding="false"
        android:onClick="toLevel"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:tag="12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/levelButton11"
        app:layout_constraintTop_toTopOf="@+id/levelButton11"
        app:srcCompat="@drawable/nor"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/levelButton14"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/round_button"
        android:backgroundTint="#000000"
        android:cropToPadding="false"
        android:onClick="toLevel"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:tag="14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/levelButton13"
        app:layout_constraintTop_toTopOf="@+id/levelButton13"
        app:srcCompat="@drawable/xnor"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/levelButton13"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/round_button"
        android:backgroundTint="#000000"
        android:cropToPadding="false"
        android:onClick="toLevel"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:tag="13"
        app:layout_constraintEnd_toStartOf="@+id/levelButton14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        app:srcCompat="@drawable/xor"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/levelButton11"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/round_button"
        android:backgroundTint="#000000"
        android:cropToPadding="false"
        android:onClick="toLevel"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:tag="11"
        app:layout_constraintEnd_toStartOf="@+id/levelButton12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/levelButton10"
        app:layout_constraintTop_toTopOf="@+id/levelButton10"
